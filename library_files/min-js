$.fn.contextMenu=function(operation){if(operation===undefined){this.first().trigger('contextmenu');}else if(operation.x&&operation.y){this.first().trigger($.Event("contextmenu",{pageX:operation.x,pageY:operation.y}));}else if(operation==="hide"){var $menu=this.data('contextMenu').$menu;$menu&&$menu.trigger('contextmenu:hide');}else if(operation){this.removeClass('context-menu-disabled');}else if(!operation){this.addClass('context-menu-disabled');}
return this;};$.contextMenu=function(operation,options){if(typeof operation!='string'){options=operation;operation='create';}
