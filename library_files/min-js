function setAvatarEditor(data){if(!data){$.getJSON("/user/getAvatarData/"+__uid,{},function(data){resetAvatarData(data);});}else{resetAvatarData(data);}}
function resetAvatarData(data){var r,coords;$("#avtr_raw").html('');$("#avtr_prev").html('');if(data){if(data.avatar_def){$("#avtr_raw").html('Upload an avatar!');$("<img />",{src:'http://cdn.morguefile.com/avatar/defavatar.gif',id:"avtr_prev_src",width:"100px",height:"100px"}).appendTo("#avtr_prev");if($('#profile_avatar').length>0){$('#profile_avatar').attr("src",'http://cdn.morguefile.com/avatar/defavatar.gif');}}else{if(data.rect){r=data.rect.split(',');coords={w:parseFloat(r[7]),h:parseFloat(r[8]),x:parseFloat(r[5]),y:parseFloat(r[6])};}else{coords={w:100,h:100,x:0,y:0};}
$("<img />",{src:data.avatar_raw,id:"avtr_prev_src"}).appendTo("#avtr_prev");$("<img />",{src:data.avatar_raw,id:"avtr_raw_src"}).appendTo("#avtr_raw");$('#avtr_raw_src').load(function(){_avtr_raw_w=$(this).width();_avtr_raw_h=$(this).height();$('#avtr_raw_src').Jcrop({onChange:showAvatarPreview,onSelect:showAvatarPreview,setSelect:[coords.x,coords.y,coords.w,coords.h],aspectRatio:1},function(){_avtr_jcrop_api=this;});});}
if(data.avatar){if($('#profile_avatar').length>0){$('#profile_avatar').attr("src",data.avatar);}
$("#profle_avatar_img").attr("src",data.avatar);$("#hdr_avatar_img").attr("src",data.avatar);}}
mwindow_removeld();}
function showAvatarPreview(coords){var rx=100/coords.w;var ry=100/coords.h;var w=Math.round(rx*_avtr_raw_w);var h=Math.round(ry*_avtr_raw_h);var x=Math.round(rx*coords.x);var y=Math.round(ry*coords.y);var s=(rx);$('#avtr_prev_src').css({width:w+'px',height:h+'px',marginLeft:'-'+x+'px',marginTop:'-'+y+'px'});_avtr_rect=s+','+x+','+y+','+w+','+h+','+coords.x+','+coords.y+','+coords.w+','+coords.h;}
function saveAvatar(id){if(_avtr_rect){mwindow_ld('#avatar-edit',true);$.getJSON("/user/saveAvatarData/"+__uid,{'rect':_avtr_rect},function(data){if(data.avatar){$("#profle_avatar_img").attr("src",data.avatar);$("#hdr_avatar_img").attr("src",data.avatar);}
mwindow_removeld();});}}
