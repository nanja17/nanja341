function getRepString(rep){repint=parseFloat(rep);if(repint<1000){return repint;}else if(repint<10000){return rep.charAt(0)+','+rep.substring(1);}else if(repint<1000000){return getRepDec((repint/1000).toFixed(repint%1000!=0))+'K';}else{return getRepDec((repint/1000000).toFixed(repint%10000000!=0))+'M';}}
function getRepDec(n){var o=n.split('.');if(parseFloat(o[1])==0){return Math.round(parseFloat(n));}
return n;}
