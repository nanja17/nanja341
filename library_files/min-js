function madd_keys(){var lst=mget_file_list();var o=new Object();var n=$('#keywords').val();if(lst&&lst.length>0){for(var i=0;i<lst.length;++i){var kys=mmergeKeys(n,$('#tgs_'+lst[i]).val());$('#tgs_'+lst[i]).val(kys);o['data[keys]['+lst[i]+']']=kys;}
