if(parseFloat(val.Archive.approved)!=1&&_lic==1){$(elm).addClass('unapprvd')}
$(elm).attr('data-orien',val.Archive.orien);$(elm).attr('data-liked',(val.Archive.liked)?1:0);$(elm).attr('data-fav',(val.Archive.fav)?1:0);$(elm).attr('data-views',val.Archive.views);$(elm).attr('data-downloads',val.Archive.downloads);$(elm).attr('data-owner',val.Archive.owner);$(elm).attr('data-likes',val.Archive.likes);$(elm).attr('data-license',_lic);$(elm).attr('data-author',val.User.username);$(elm).attr('data-id',_id);$(elm).mouseover(function(evt){evt.stopPropagation();mroll(_id,this);});return elm;}
function mroll(_id,_this){if(_mroll){_mroll.remove();}
clearTimeout(_mroll_tme);_mroll_tme=setTimeout(function(){var _xtra,desc;var p=$(_this).position();_mroll=$(_this).clone();$(_mroll).addClass('mthumb_roll').attr('id','mroll');$(_mroll).css('top',p.top);$(_mroll).css('left',p.left);$('body').append(_mroll);var _w=360;var _h=390;var _h2=340;var _l=p.left-85;var _t=p.top-100;if($.browser.msie&&parseFloat($.browser.version)<9){_w=300;_h=350;_h2=300
_l=p.left-70;_t=p.top-80;}
