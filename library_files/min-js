function mmergeKeys(a,arr){var keys=arr.split(',');keys.push(a);var tmp=new Array();for(var i=0;i<keys.length;++i){var k=jQuery.trim(keys[i]);k=k.replace('"',"");k=k.replace("'","");if(k.length>0&&k!=' '&&k!=undefined){tmp.push(k);}}
jQuery.unique(tmp);return tmp.join(', ');}
