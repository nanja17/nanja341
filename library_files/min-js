if($(_this).attr('data-license')>100){_xtra='';switch($(_this).attr('data-license')){case'999':desc=$('<div><\/div>').addClass('mthumb_descr').html('<span class="author">Dreamstime id: '+$(_this).attr('data-id')+'<\/span><span class="stats">downloads: '+$(_this).attr('data-downloads')+'<\/span><div class="icons"><a onclick="favimg(\''+_id+'\', \'dream\');"><img src="'+favImg+'" id="favicn_'+_id+'"><\/a><\/div>');break;case'998':desc=$('<div><\/div>').addClass('mthumb_descr').html('<span class="author">'+$(_this).attr('data-desc')+'<\/span><div class="icons"><a onclick="favimg(\''+_id+'\', \'shuttr\');"><img src="'+favImg+'" id="favicn_'+_id+'"><\/a><\/div>');break;case'997':desc=genRollDescr('fotolia',_this,_id,favImg);break;case'995':desc=genRollDescr('depositphotos',_this,_id,favImg);break;case'994':desc=genRollDescr('crestock',_this,_id,favImg);break;case'990':desc=genRollDescr('stockfresh',_this,_id,favImg);break;case'996':desc=genRollDescr('istock',_this,_id,favImg);break;case'992':desc=genRollDescr('123rf',_this,_id,favImg);break;case'989':desc=genRollDescr('getty',_this,_id,favImg);break;case'890':desc=genRollDescr('bigstock',_this,_id,favImg);break;}}else{_xtra=(parseFloat($(_this).attr('data-license'))==1||parseFloat($(_this).attr('data-license'))==99)?'<a onclick="mdownload(\''+_id+'\');"><img src="/img/download.png"><\/a><a onclick="crop(\''+_id+'\');"><img src="/img/crop.png"><\/a>':'';desc=$('<div><\/div>').addClass('mthumb_descr').html('<span class="author"><a href="/creative/'+$(_this).attr('data-author')+'">'+$(_this).attr('data-author')+'</a><\/span><span class="stats">'+$(_this).attr('data-views')+' | '+$(_this).attr('data-downloads')+' | '+$(_this).attr('data-likes')+'<\/span><div class="icons"><a onclick="favimg(\''+_id+'\');"><img src="'+favImg+'" id="favicn_'+_id+'"><\/a><a onclick="likeimg(\''+_id+'\');"><img src="'+likeImg+'" id="likeicn_'+_id+'"><\/a>'+_xtra+'<\/div>');}
$('#mroll').append(desc);$(desc).hide();$(desc).fadeIn('fast').delay(100);$('.mthumb_brdr').mouseover(function(evt){evt.stopPropagation();});},300);}
