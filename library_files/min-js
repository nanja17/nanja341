var crp_file_id='0';var crp_file_width=parseFloat('0');var crp_file_height=parseFloat('0');var crp_x=0;var crp_y=0;var crp_w=parseFloat('0');var crp_h=parseFloat('0');var crp_s=30;var calc_x=0;var calc_y=0;var calc_w=parseFloat('0');var calc_h=parseFloat('0');var calc_s=30;var api;var lock_size=false;function crop(n){var _this=this;var o=new Object();o.title='Crop Image.';o.width=890;o.height=595;o.html='<div id="cropAreaTool"><div id="cropAreaNst"><span id="cropArea"></span></div>\
   <div id="cropTools">\
    <ul>\
     <li>Width: <span id="coord_w">0</span> pixels</li>\
     <li>Height: <span id="coord_h">0</span> pixels</li>\
     <li>Lock size: <input name="lck_sz" id="lck_sz" type="checkbox" onchange="croplockSize();"></li>\
     <li>Scale: <div id="sldr_scale" class="ui-slider ui-slider-horizontal ui-widget ui-widget-content ui-corner-all"><a href="#" class="ui-slider-handle ui-state-default ui-corner-all"></a></div><span id="coord_s">0</span></li>\
    </ul>\
    <blockquote>Images will not be sized larger than 1000 pixels</blockquote>\
    <div class="buttons">\
     <span class="mbtn_glbl_orng" onclick="doTheCrop();">Finish</span> <span class="mbtn_glbl_orng" onclick="mwindow();">Cancel</span>\
    </div>\
   </div></div>';mwindow(o,function(){$.post("/archive/image/"+n,{},function(data){crp_file_height=parseFloat(data.Archive.file_height);crp_file_width=parseFloat(data.Archive.file_width);crp_file_id=n;var im=$('<img src="'+data.Archive.file_path_large+'" id="cropbox">').on('load',function(){var w=im.width()*.85;var h=im.height()*.85;im.width(w);im.height(h);$('#cropArea').css('width',w);$('#cropArea').css('height',h);function restSelct(){api.setSelect([crp_x,crp_y,(crp_x+crp_w),(crp_y+crp_h)]);api.setOptions({aspectRatio:getAspectRatio()});}
api=$.Jcrop('#cropbox',{setSelect:[20,20,400,400],onSelect:_this.onSlct,onChange:_this.updteCrp,trueSize:[crp_file_width,crp_file_height]});$('#sldr_scale').slider({min:.3,max:201,slide:function(){crp_s=parseFloat($('#sldr_scale').slider('option','value'));$('#coord_s').text(parseInt(crp_s)+'%');if(lock_size!=true){updateCalc();}else{updateReCalc();}},value:crp_s});_this.updateCalc();});$('#cropArea').append(im);},'json');});}
function onSlct(obj){if(obj.w==0||obj.h==0){crp_x=0;crp_y=0;crp_w=crp_file_width;crp_h=crp_file_height;$('#lck_sz').attr('checked',false);api.setOptions({aspectRatio:0});lock_size=false;}}
function updteCrp(obj){if(obj.w==0||obj.h==0){crp_x=0;crp_y=0;crp_w=crp_file_width;crp_h=crp_file_height;}else{crp_x=obj.x;crp_y=obj.y;crp_w=obj.w;crp_h=obj.h;}
updateCalc();}
function updateCalc(){if(lock_size==true){crp_s=(calc_w/crp_w)*100;$('#sldr_scale').slider('option','value',crp_s);}else{calc_x=parseInt((crp_x*crp_s)/100);calc_y=parseInt((crp_y*crp_s)/100);calc_w=parseInt((crp_w*crp_s)/100);calc_h=parseInt((crp_h*crp_s)/100);$('#coord_w').text(parseInt(calc_w));$('#coord_h').text(parseInt(calc_h));}
$('#coord_s').text(parseInt(crp_s)+'%');}
function updateReCalc(){calc_x=parseInt((crp_x*crp_s)/100);calc_y=parseInt((crp_y*crp_s)/100);calc_w=parseInt((crp_w*crp_s)/100);calc_h=parseInt((crp_h*crp_s)/100);$('#coord_w').text(parseInt(calc_w));$('#coord_h').text(parseInt(calc_h));api.setSelect([crp_x,crp_y,(crp_x+crp_w),(crp_y+crp_h)]);}
function croplockSize(){lock_size=($('#lck_sz').attr('checked')=='checked')?true:false;api.setOptions({aspectRatio:getAspectRatio()});}
function getAspectRatio(){if(lock_size!=true){return 0;}else{if(crp_w==crp_h){return 1;}else{return crp_w/crp_h;}}}
function doTheCrop(){var crp=new Object();crp['unique_id']=crp_file_id;crp['crp_x']=crp_x;crp['crp_y']=crp_y;crp['crp_w']=crp_w;crp['crp_h']=crp_h;crp['crp_s']=crp_s;crp['calc_w']=$('#coord_w').text();crp['calc_h']=$('#coord_h').text();$('#cropAreaTool').html('');$('#cropAreaTool').addClass('bigLoader');$.post("/archive/crop/",crp,function(data){var imgretrnd='';if(data.calc_w>600||data.calc_h>600){imgretrnd='<span class="img_too_lrg">The image is too large to be displayed in this space.</span>';}else{imgretrnd='<span><a href="'+data.tny_url+'"><img src="'+data.tny_url+'"></a></span>';}
$('#cropAreaTool').removeClass('bigLoader');$('#cropAreaTool').html('<div id="cropAreaNst"><div class="cropPreview" id="cropArea">'+imgretrnd+'</div></div><div id="cropTools" class="cropResults"><h1>Direct link to image:</h1><p>Use this link to link to the image file directly.<br><a href="'+data.tny_url+'">'+data.tny_url+'</a></p><h1>Html for posting:</h1><p>This html code has the image and photo credit you can readily use in a blog post.<br><textarea id="morguefile_img_html_code"><img src="'+data.tny_url+'" width="'+data.calc_w+'" height="'+data.calc_h+'" border="0"><br>Photo credit: <a href="http://www.morguefile.com/creative/'+data.auth+'">'+data.auth+'</a> from <a href="http://www.morguefile.com/">morguefile.com</a></textarea></p></div>');},'json');}
