function randomString(n){var chars="0123456789ABCDEFGHIJKLMNOPQRSTUVWXTZabcdefghiklmnopqrstuvwxyz";var string_length=(n)?n:8;var randomstring='';for(var i=0;i<string_length;i++){var rnum=Math.floor(Math.random()*chars.length);randomstring+=chars.substring(rnum,rnum+1);}
document.randform.randomfield.value=randomstring;}
function setCookie(n,m){var myDate=new Date();myDate.setMonth(myDate.getMonth()+12);document.cookie=n+"="+m+";expires="+myDate+";domain=.morguefile.com;path=/";}
function getCookie(name){var value="; "+document.cookie;var parts=value.split("; "+name+"=");if(parts.length==2)return parts.pop().split(";").shift();}
